{
    "name": "modryd/mapquery",
    "description": "PHP library for searching Points of Interest (POI) by coordinates using Overpass API",
    "type": "library",
    "keywords": [
        "poi",
        "nominatim",
        "overpass",
        "openstreetmap",
        "geocoding",
        "coordinates",
        "search",
        "map",
        "location"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Modr√Ω",
            "email": "github-modryd@podnika.cz"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "modryd\\MapQuery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "modryd\\MapQuery\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
